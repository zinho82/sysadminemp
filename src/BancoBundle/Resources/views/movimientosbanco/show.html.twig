{% extends 'AppBundle:Layouts:layout.html.twig' %}

{% block title %}
{{ parent() }} - movimientosBanco
{% endblock %}

{% block body %}
<div class="col-lg-12 col-sm-12 col-md-12">

<div id="top" class="row">
    <div class="page-header">
        <h2>View movimientosBanco <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ movimientosBanco.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Detalle</strong></p>
        <p>
            {{ movimientosBanco.detalle }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fecha</strong></p>
        <p>
            {% if movimientosBanco.fecha %}{{ movimientosBanco.fecha|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fechaingreso</strong></p>
        <p>
            {% if movimientosBanco.fechaIngreso %}{{ movimientosBanco.fechaIngreso|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Monto</strong></p>
        <p>
            {{ movimientosBanco.monto }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Numerooperacion</strong></p>
        <p>
            {{ movimientosBanco.numeroOperacion }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('movimientosbanco') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('movimientosbanco_edit', { 'id':  movimientosBanco.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('movimientosbanco_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('movimientosbanco_delete', { 'id': movimientosBanco.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>

</div>
{% endblock %}

