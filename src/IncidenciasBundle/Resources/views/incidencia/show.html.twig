{% extends 'AppBundle:Layouts:layout.html.twig' %}

{% block title %}
{{ parent() }} - incidencium
{% endblock %}

{% block body %}
<div class="col-lg-12 col-sm-12 col-md-12">
<div id="top" class="row">
    <div class="page-header">
        <h2>View incidencium <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ incidencium.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fechaincidencia</strong></p>
        <p>
            {% if incidencium.fechaIncidencia %}{{ incidencium.fechaIncidencia|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Numeroincidencia</strong></p>
        <p>
            {{ incidencium.numeroIncidencia }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Descripcionerror</strong></p>
        <p>
            {{ incidencium.descripcionError }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fechasolucion</strong></p>
        <p>
            {% if incidencium.fechaSolucion %}{{ incidencium.fechaSolucion|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Solucion</strong></p>
        <p>
            {{ incidencium.solucion }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fechaingreso</strong></p>
        <p>
            {% if incidencium.fechaIngreso %}{{ incidencium.fechaIngreso|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Archivo</strong></p>
        <p>
            {{ incidencium.archivo }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('incidencia') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('incidencia_edit', { 'id':  incidencium.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('incidencia_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('incidencia_delete', { 'id': incidencium.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>

</div>
{% endblock %}

