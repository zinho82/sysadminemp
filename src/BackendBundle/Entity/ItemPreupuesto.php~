<?php

namespace BackendBundle\Entity;

/**
 * ItemPreupuesto
 */
class ItemPreupuesto
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nombre;

    /**
     * @var float
     */
    private $valor;

    /**
     * @var string
     */
    private $descripcion;

    /**
     * @var string
     */
    private $duracionMeses;

    /**
     * @var string
     */
    private $mesInicio;

    /**
     * @var string
     */
    private $anoInicio;

    /**
     * @var string
     */
    private $prorateable;

    /**
     * @var \BackendBundle\Entity\Presupuesto
     */
    private $presupuesto;

    /**
     * @var \BackendBundle\Entity\Config
     */
    private $tipoIngresoGasto;

    /**
     * @var \BackendBundle\Entity\Campana
     */
    private $campana;

    /**
     * @var \BackendBundle\Entity\Config
     */
    private $itemGasto;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nombre
     *
     * @param string $nombre
     *
     * @return ItemPreupuesto
     */
    public function setNombre($nombre)
    {
        $this->nombre = $nombre;

        return $this;
    }

    /**
     * Get nombre
     *
     * @return string
     */
    public function getNombre()
    {
        return $this->nombre;
    }

    /**
     * Set valor
     *
     * @param float $valor
     *
     * @return ItemPreupuesto
     */
    public function setValor($valor)
    {
        $this->valor = $valor;

        return $this;
    }

    /**
     * Get valor
     *
     * @return float
     */
    public function getValor()
    {
        return $this->valor;
    }

    /**
     * Set descripcion
     *
     * @param string $descripcion
     *
     * @return ItemPreupuesto
     */
    public function setDescripcion($descripcion)
    {
        $this->descripcion = $descripcion;

        return $this;
    }

    /**
     * Get descripcion
     *
     * @return string
     */
    public function getDescripcion()
    {
        return $this->descripcion;
    }

    /**
     * Set duracionMeses
     *
     * @param string $duracionMeses
     *
     * @return ItemPreupuesto
     */
    public function setDuracionMeses($duracionMeses)
    {
        $this->duracionMeses = $duracionMeses;

        return $this;
    }

    /**
     * Get duracionMeses
     *
     * @return string
     */
    public function getDuracionMeses()
    {
        return $this->duracionMeses;
    }

    /**
     * Set mesInicio
     *
     * @param string $mesInicio
     *
     * @return ItemPreupuesto
     */
    public function setMesInicio($mesInicio)
    {
        $this->mesInicio = $mesInicio;

        return $this;
    }

    /**
     * Get mesInicio
     *
     * @return string
     */
    public function getMesInicio()
    {
        return $this->mesInicio;
    }

    /**
     * Set anoInicio
     *
     * @param string $anoInicio
     *
     * @return ItemPreupuesto
     */
    public function setAnoInicio($anoInicio)
    {
        $this->anoInicio = $anoInicio;

        return $this;
    }

    /**
     * Get anoInicio
     *
     * @return string
     */
    public function getAnoInicio()
    {
        return $this->anoInicio;
    }

    /**
     * Set prorateable
     *
     * @param string $prorateable
     *
     * @return ItemPreupuesto
     */
    public function setProrateable($prorateable)
    {
        $this->prorateable = $prorateable;

        return $this;
    }

    /**
     * Get prorateable
     *
     * @return string
     */
    public function getProrateable()
    {
        return $this->prorateable;
    }

    /**
     * Set presupuesto
     *
     * @param \BackendBundle\Entity\Presupuesto $presupuesto
     *
     * @return ItemPreupuesto
     */
    public function setPresupuesto(\BackendBundle\Entity\Presupuesto $presupuesto = null)
    {
        $this->presupuesto = $presupuesto;

        return $this;
    }

    /**
     * Get presupuesto
     *
     * @return \BackendBundle\Entity\Presupuesto
     */
    public function getPresupuesto()
    {
        return $this->presupuesto;
    }

    /**
     * Set tipoIngresoGasto
     *
     * @param \BackendBundle\Entity\Config $tipoIngresoGasto
     *
     * @return ItemPreupuesto
     */
    public function setTipoIngresoGasto(\BackendBundle\Entity\Config $tipoIngresoGasto = null)
    {
        $this->tipoIngresoGasto = $tipoIngresoGasto;

        return $this;
    }

    /**
     * Get tipoIngresoGasto
     *
     * @return \BackendBundle\Entity\Config
     */
    public function getTipoIngresoGasto()
    {
        return $this->tipoIngresoGasto;
    }

    /**
     * Set campana
     *
     * @param \BackendBundle\Entity\Campana $campana
     *
     * @return ItemPreupuesto
     */
    public function setCampana(\BackendBundle\Entity\Campana $campana = null)
    {
        $this->campana = $campana;

        return $this;
    }

    /**
     * Get campana
     *
     * @return \BackendBundle\Entity\Campana
     */
    public function getCampana()
    {
        return $this->campana;
    }

    /**
     * Set itemGasto
     *
     * @param \BackendBundle\Entity\Config $itemGasto
     *
     * @return ItemPreupuesto
     */
    public function setItemGasto(\BackendBundle\Entity\Config $itemGasto = null)
    {
        $this->itemGasto = $itemGasto;

        return $this;
    }

    /**
     * Get itemGasto
     *
     * @return \BackendBundle\Entity\Config
     */
    public function getItemGasto()
    {
        return $this->itemGasto;
    }
}
