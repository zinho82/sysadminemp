<?php

namespace BackendBundle\Entity;

/**
 * Perfil
 */
class Perfil
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var \BackendBundle\Entity\Menu
     */
    private $menu;

    /**
     * @var \BackendBundle\Entity\Config
     */
    private $tipoPerfil;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set menu
     *
     * @param \BackendBundle\Entity\Menu $menu
     *
     * @return Perfil
     */
    public function setMenu(\BackendBundle\Entity\Menu $menu = null)
    {
        $this->menu = $menu;

        return $this;
    }

    /**
     * Get menu
     *
     * @return \BackendBundle\Entity\Menu
     */
    public function getMenu()
    {
        return $this->menu;
    }

    /**
     * Set tipoPerfil
     *
     * @param \BackendBundle\Entity\Config $tipoPerfil
     *
     * @return Perfil
     */
    public function setTipoPerfil(\BackendBundle\Entity\Config $tipoPerfil = null)
    {
        $this->tipoPerfil = $tipoPerfil;

        return $this;
    }

    /**
     * Get tipoPerfil
     *
     * @return \BackendBundle\Entity\Config
     */
    public function getTipoPerfil()
    {
        return $this->tipoPerfil;
    }
}
