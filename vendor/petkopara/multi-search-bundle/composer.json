{
    "name": "petkopara/multi-search-bundle",
    "description": "Symfony3 bundle that provides multi search doctrine query builder for forms and entities .",
    "keywords": [ "multi-search", "query-builder", "symfony3", "doctrine"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/petkopara/PetkoparaMultiSearchBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Petko Petkov",
            "email": "petkopara@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": "~2.8|~3.0",
        "symfony/form": "~2.8|~3.0",
        "doctrine/doctrine-bundle": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Petkopara\\MultiSearchBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
